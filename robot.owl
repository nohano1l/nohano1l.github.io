<?xml version="1.0"  encoding="UTF-8"?>

<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:base="http://example.com/robot/">

  <!-- Classes -->
  <owl:Class rdf:ID="Robot"/>
  <owl:Class rdf:ID="IndustrialRobot">
    <rdfs:subClassOf rdf:resource="#Robot"/>
  </owl:Class>
  <owl:Class rdf:ID="MobileRobot">
    <rdfs:subClassOf rdf:resource="#Robot"/>
  </owl:Class>
  <owl:Class rdf:ID="HumanoidRobot">
    <rdfs:subClassOf rdf:resource="#Robot"/>
  </owl:Class>
  <owl:Class rdf:ID="ArticulatedRobot">
    <rdfs:subClassOf rdf:resource="#IndustrialRobot"/>
  </owl:Class>
  <owl:Class rdf:ID="SCARARobot">
    <rdfs:subClassOf rdf:resource="#IndustrialRobot"/>
  </owl:Class>
  <owl:Class rdf:ID="DeltaRobot">
    <rdfs:subClassOf rdf:resource="#IndustrialRobot"/>
  </owl:Class>
  <owl:Class rdf:ID="WheeledRobot">
    <rdfs:subClassOf rdf:resource="#MobileRobot"/>
  </owl:Class>
  <owl:Class rdf:ID="LeggedRobot">
    <rdfs:subClassOf rdf:resource="#MobileRobot"/>
  </owl:Class>
  <owl:Class rdf:ID="TrackedRobot">
    <rdfs:subClassOf rdf:resource="#MobileRobot"/>
  </owl:Class>
  <owl:Class rdf:ID="AndroidRobot">
    <rdfs:subClassOf rdf:resource="#HumanoidRobot"/>
  </owl:Class>
  <owl:Class rdf:ID="BipedalRobot">
    <rdfs:subClassOf rdf:resource="#HumanoidRobot"/>
  </owl:Class>
  <owl:Class rdf:ID="QuadrupedalRobot">
    <rdfs:subClassOf rdf:resource="#HumanoidRobot"/>
  </owl:Class>

  <!-- Properties -->
  <owl:SymmetricProperty rdf:ID="hasSibling">
    <rdfs:domain rdf:resource="#Robot"/>
    <rdfs:range rdf:resource="#Robot"/>
  </owl:SymmetricProperty>

  <owl:TransitiveProperty rdf:ID="containedIn">
    <rdfs:domain rdf:resource="#QuadrupedalRobot"/>
    <rdfs:range rdf:resource="#HumanoidRobot"/>
  </owl:TransitiveProperty>

  <owl:InverseProperty rdf:ID="contains">
    <owl:inverseOf rdf:resource="#containedIn"/>
    <rdfs:domain rdf:resource="#HumanoidRobot"/>
    <rdfs:range rdf:resource="#QuadrupedalRobot"/>
  </owl:InverseProperty>

  <owl:FunctionalProperty rdf:ID="isTypeOf">
    <rdfs:domain rdf:resource="#ArticulatedRobot"/>
    <rdfs:range rdf:resource="#IndustrialRobot"/>
  </owl:FunctionalProperty>

  <!-- <owl:ObjectProperty rdf:about="http://example.com/robot/hasType">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
    <owl:inverseOf rdf:resource="http://example.com/robot/isTypeOf"/>
  </owl:ObjectProperty> -->

  <owl:InverseFunctionalProperty rdf:ID="hasType">
    <owl:inverseOf rdf:resource="#isTypeOf"/>
    <rdfs:domain rdf:resource="#IndustrialRobot"/>
    <rdfs:range rdf:resource="#ArticulatedRobot"/>
  </owl:InverseFunctionalProperty>

</rdf:RDF>